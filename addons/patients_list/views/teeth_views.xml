<odoo>
    <record id="teeth_form" model="ir.ui.view">
        <field name="name">teeth.form</field>
        <field name="model">teeth</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="description"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="teeth_list" model="ir.ui.view">
        <field name="name">teeth.list</field>
        <field name="model">teeth</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="description"/>
            </list>
        </field>
    </record>

    <record id="teeth_list_action" model="ir.actions.act_window">
        <field name="name">Teeth list</field>
        <field name="res_model">teeth</field>
        <field name="view_mode">list,form</field>
    </record>

    <menuitem id="teeth_list_sub"
              name="Teeth"
              parent="configuration_list_main"
              action="teeth_list_action"
              sequence="250"/>
</odoo>