<odoo>
    <record id="medical_history_form" model="ir.ui.view">
        <field name="name">medical.history.form</field>
        <field name="model">medical.history</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name"/>
                </group>
            </form>
        </field>
    </record>
    <record id="medical_history_list" model="ir.ui.view">
        <field name="name">medical.history.list</field>
        <field name="model">medical.history</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <record id="medical_history_action" model="ir.actions.act_window">
        <field name="name">Medical history list</field>
        <field name="res_model">medical.history</field>
        <field name="view_mode">list,form</field>
    </record>

    <menuitem id="medical_history_sub"
              name="Medical history"
              parent="patient_list_main"
              action="medical_history_action"
              sequence="200"/>


</odoo>